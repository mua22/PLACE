package epclaim.compiler;

import java.util.ArrayList;

/**
 * @author Usman
 * Creates a Tree Like Structure for nested Conditions. 
 *
 */
public class ConditionType {
	
	/*
	 * type 
	 * o for none
	 * 1 for and
	 * 2 for or
	 */
	private int type = 0;
	private ArrayList<ConditionType> childTypes;
	private ArrayList<FunctionSignature> condition;
	
	
}
