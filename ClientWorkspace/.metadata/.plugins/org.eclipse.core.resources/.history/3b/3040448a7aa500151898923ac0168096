
public class Sorting {
	public static int[] arrOf = {2,8,3,6,1};
 public static void main(String args[]){
	 printarr();
	 for(int i=0;i<5;i++){
		 
	 
		 for(int j=0;j<5;++j){
			 if(arrOf[i]>arrOf[i+1]){
				 int temp = arrOf[i+1];
				 arrOf[i] = arrOf[i+1];
				 arrOf[i+1] = temp;
				 
			 }
		 } printarr();
		 }
 }
 public static void printarr(){
	 String str = "";
	 for(int i=0;i<5;i++)
		 str+=arrOf[i] + " ";
	 System.out.println(str);
 }
}
